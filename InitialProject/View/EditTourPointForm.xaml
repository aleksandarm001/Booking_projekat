<Window x:Class="InitialProject.View.EditTourPointForm"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:InitialProject.View"
        mc:Ignorable="d"
        Title="EditTourPointForm" Height="64" Width="300" WindowStyle="ToolWindow" WindowStartupLocation="CenterScreen">
    <Grid>
        <Label x:Name="FirstName" Margin="0,10,0,-10">dsa</Label>
        <ComboBox Name ="CityComboBox" IsTextSearchEnabled="True" StaysOpenOnEdit="True" IsEditable="True"
                      ItemsSource="{Binding Orders}" HorizontalAlignment="Left" Width ="33" SelectedIndex="0" Margin="196,10,0,10"/>
        <Button Margin="247,10,6,10" Click="Save_ButtonClick">Sacuvaj</Button>
    </Grid>
</Window>
